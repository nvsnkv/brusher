{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "one-line": false,
        "no-trailing-whitespace": [true, "ignore-blank-lines"],
        "no-empty": false,
        "member-ordering": false,
        "member-access": false,
        "trailing-comma": false,
        "triple-equals": false,
        "object-literal-sort-keys": false,
        "max-line-length": false
    },
    "rulesDirectory": []
}